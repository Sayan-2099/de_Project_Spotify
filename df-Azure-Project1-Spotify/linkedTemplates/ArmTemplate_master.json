{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-Azure-Project1-Spotify"
		},
		"azureproject1_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azureproject1_db'"
		},
		"azureprojectsink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azureprojectsink'"
		},
		"azureproject1_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azureproject-1server.database.windows.net"
		},
		"azureproject1_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azureprojectdb1"
		},
		"azureproject1_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sayan_devcode"
		},
		"azureprojectsink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://project1storagespotify.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-Azure-Project1-Spotify_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azureproject1_db_password": {
						"value": "[parameters('azureproject1_db_password')]"
					},
					"azureprojectsink_accountKey": {
						"value": "[parameters('azureprojectsink_accountKey')]"
					},
					"azureproject1_db_properties_typeProperties_server": {
						"value": "[parameters('azureproject1_db_properties_typeProperties_server')]"
					},
					"azureproject1_db_properties_typeProperties_database": {
						"value": "[parameters('azureproject1_db_properties_typeProperties_database')]"
					},
					"azureproject1_db_properties_typeProperties_userName": {
						"value": "[parameters('azureproject1_db_properties_typeProperties_userName')]"
					},
					"azureprojectsink_properties_typeProperties_url": {
						"value": "[parameters('azureprojectsink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}